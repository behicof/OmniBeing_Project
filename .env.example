# Environment Variables for OmniBeing Trading System
# Copy this file to .env and fill in your actual values

# API Keys (Keep these secure!)
MARKET_DATA_API_KEY=your_market_data_api_key_here
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_SECRET_KEY=your_binance_secret_key_here

# Trading Configuration (Optional - will use config.yaml if not set)
TRADING_INSTRUMENT=XAUUSD
INITIAL_CAPITAL=10000
RISK_PERCENTAGE=1.5

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO